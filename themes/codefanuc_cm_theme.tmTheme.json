      {
        "scope": "punctuation.delimiter.udl-5.fanuctp_ls",
        "settings": {
          "foreground": "#804000"
        }
      },
      {
        "scope": "punctuation.delimiter.udl-6.fanuctp_ls",
        "settings": {
          "foreground": "#800000"
        }
      },
      {
        "scope": "punctuation.delimiter.udl-7.fanuctp_ls",
