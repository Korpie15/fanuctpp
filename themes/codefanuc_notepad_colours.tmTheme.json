{
    "name": "Fanuc Theme (Simplified UDL)",
    "type": "vs-dark",
    "colors": {
      "editor.foreground": "#FFFFFF",
      "editor.background": "#000000"
    },
    "tokenColors": [
      {
        "scope": "source.fanuctp_ls punctuation.bracket.square.character.fanuctp_ls",
        "settings": {
          "foreground": "#FF9B37"
        }
      },
      {
        "scope": "source.fanuctp_ls meta.content.bracket.square.fanuctp_ls",
        "settings": {
          "foreground": "#FF9B37"
        }
      },
      {
        "scope": "source.fanuctp_ls punctuation.bracket.parenthesis.character.fanuctp_ls",
        "settings": {
          "foreground": "#FFA346"
        }
      },
      {
        "scope": "source.fanuctp_ls meta.content.bracket.parenthesis.fanuctp_ls",
        "settings": {
          "foreground": "#FFA346"
        }
      },
      {
        "scope": [
          "punctuation.bracket",
          "punctuation.section",
          "punctuation.other",
          "punctuation.square",
          "punctuation.parenthesis"
        ],
        "settings": {
          "foreground": "#FFA346" 
        }
      },
      {
        "scope": [
          "comment.line.udl-00.fanuctp_ls",
          "comment.line.udl-01.fanuctp_ls",
          "comment.line.udl-02.fanuctp_ls",
          "comment.line.udl-03.fanuctp_ls",
          "comment.line.udl-04.fanuctp_ls",
          "comment.line.remark.fanuctp_ls",
          "comment.line.slash.fanuctp_ls"
        ],
        "settings": {
          "foreground": "#00FF80"
        }
      },
      {
        "scope": "comment.line.double-dash.fanuctp_ls",
        "settings": {
          "foreground": "#00FF00"
        }
      },
      {
        "scope": "meta.default.fanuctp_ls",
        "settings": {
          "foreground": "#FFFFFF",
          "background": "#000000"
        }
      },
      {
        "scope": "constant.numeric.fanuctp_ls",
        "settings": {
          "foreground": "#DF00FF"
        }
      },
      {
        "scope": "keyword.operator.fanuctp_ls",
        "settings": {
          "foreground": "#D500D5"
        }
      },
      {
        "scope": "keyword.udl-1.fanuctp_ls",
        "settings": {
          "foreground": "#FF0000"
        }
      },
      {
        "scope": "keyword.udl-2.fanuctp_ls",
        "settings": {
          "foreground": "#00FFFF",
          "fontStyle": "bold underline"
        }
      },
      {
        "scope": "keyword.udl-3.fanuctp_ls",
        "settings": {
          "foreground": "#8000FF"
        }
      },
      {
        "scope": "keyword.udl-4.fanuctp_ls",
        "settings": {
          "foreground": "#1D61FE",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "punctuation.delimiter.udl-1.fanuctp_ls",
        "settings": {
          "foreground": "#FF9B37"
        }
      },
      {
        "scope": "punctuation.delimiter.udl-2.fanuctp_ls",
        "settings": {
          "foreground": "#BB5E00"
        }
      },
      {
        "scope": "punctuation.delimiter.udl-3.fanuctp_ls",
        "settings": {
          "foreground": "#FFA346"
        }
      },
      {
        "scope": "punctuation.delimiter.udl-4.fanuctp_ls",
        "settings": {
          "foreground": "#C16100"
        }
      },
      {
        "scope": "punctuation.delimiter.udl-5.fanuctp_ls",
        "settings": {
          "foreground": "#804000"
        }
      },
      {
        "scope": "punctuation.delimiter.udl-6.fanuctp_ls",
        "settings": {
          "foreground": "#800000"
        }
      },
      {
        "scope": "punctuation.delimiter.udl-7.fanuctp_ls",
        "settings": {
          "foreground": "#808000"
        }
      },
      {
        "scope": "punctuation.delimiter.udl-8.fanuctp_ls",
        "settings": {
          "foreground": "#808040"
        }
      },
      {
        "scope": "punctuation.definition.string.quote.fanuctp_ls",
        "settings": {
          "foreground": "#8000FF"
        }
      },
      {
        "scope": [
          "string.quoted.single.fanuctp_ls",
          "string.quoted.double.fanuctp_ls"
        ],
        "settings": {
          "foreground": "#FFFFFF"
        }
      },
      {
        "scope": [
          "keyword",
          "variable",
          "entity",
          "support",
          "meta",
          "storage",
          "markup",
          "invalid",
          "text",
          "source",
          "constant"
        ],
        "settings": {
          "foreground": "#FFFFFF"
        }
      }
    ]
}
